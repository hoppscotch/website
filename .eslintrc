{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:vue/vue3-recommended", "prettier"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "plugins": ["vue", "html", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "vue/multi-word-component-names": "off"
  }
}
